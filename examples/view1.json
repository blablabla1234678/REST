{
    "People": {
        "label": "People",
        "output": {
            "type": "Table",
            "columns": [
                "id",
                "name",
                "age",
                "gender"
            ]
        }
    },
    "listPeople": {
        "label": "List People"
    },
    "Person": {
        "label": "Person",
        "name": {
            "value": ["name"]
        }
    },
    "registerPerson": {
        "type": "Form",
        "label": "Create Person",
        "inputs": [
            "name",
            "age",
            "gender"
        ],
        "submit": "register"
    },
    "Name": {
        "label": "Name",
        "input": "text",
        "output": "text"
    },
    "Age": {
        "label": "Age",
        "input": "number",
        "output": "text"
    },
    "Gender": {
        "label": "Gender",
        "input": "select",
        "output": "text",
        "alternatives": [
            {"value": 1, "label": "Male"},
            {"value": 2, "label": "Female"},
            {"value": 3, "label": "Other"}
        ]
    }
}